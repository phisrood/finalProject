<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
				 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CyberCamOnlineContentsDAO">
	<insert id="insertOnlineCon" parameterType="onlineContentsVO">
		INSERT INTO ONLINE_CONTENTS (OC_NO,OC_LEC_NO,OC_CONTENT,OC_STARTDATE,OC_ENDDATE,OC_TIME,OC_DATE)
		VALUES (online_contents_seq.nextval,11,#{oc_content},sysdate,sysdate,#{oc_time},sysdate) 
	</insert>

</mapper>